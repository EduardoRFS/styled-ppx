{
  "name": "re_styled_ppx",
  "version": "0.1.0",
  "description": "",
  "author": "David Sancho",
  "license": "MIT",
  "homepage": "https://github.com/davesnx/re-styled-ppx",
  "bugs": { "url": "https://github.com/davesnx/re-styled-ppx/issues" },
  "repository": {
    "type": "git",
    "url": "https://github.com/davesnx/re-styled-ppx.git"
  },
  "esy": {
    "build": "dune build -p #{self.name}",
    "buildEnv": { "ODOC_SYNTAX": "re" }
  },
  "dependencies": {
    "@esy-ocaml/reason": "3.5.2",
    "@opam/dune": "1.11.4",
    "@opam/menhir": "20190924",
    "@opam/ocaml-migrate-parsetree": "1.5.0",
    "@opam/sedlex": "2.1",
    "@reason-native/console": "^0.1.0",
    "ocaml": "~4.6.1000"
  },
  "resolution": { "@opam/sedlex": "ocaml-community/sedlex" },
  "devDependencies": { "@opam/merlin": "*", "ocaml": "~4.6.1000" },
  "scripts": {
    "test": "dune runtest",
    "test:promote": "dune runtest --auto-promote",
    "test:regen":
      "sh -c \"refmt ./test/simple_src.re -p ml > ./test/simple.ml\"",
    "doc": "esy dune build @doc",
    "doc-path": "esy echo #{self.target_dir}/default/_doc/_html/index.html",
    "format": "esy dune build @fmt --auto-promote",
    "watch": "esy dune build -p #{self.name} --watch",
    "utop": "esy dune utop lib -- -implicit-bindings"
  }
}