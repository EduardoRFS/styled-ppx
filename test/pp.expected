module Component =
  struct
    type makeProps =
      {
      key: string [@bs.optional ];
      suppressContentEditableWarning: bool [@bs.optional ]}[@@bs.deriving
                                                             abstract]
    external setClassName : makeProps -> string -> unit = "className"
    [@@bs.set ]
    let styles = let open Emotion in css [display `block]
    let make ?children  (props : makeProps) =
      ((setClassName props styles;
        React.createElement (("div")[@reason.raw_literal "div"]) ~props
          [|((match children with
              | ((Some (chil))[@explicit_arity ]) -> chil
              | None -> React.null));[]|])
      [@reason.preserve_braces ])
  end
